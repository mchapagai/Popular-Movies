<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MaterialTextView">
        <attr name="linkColor" format="reference|color" />
    </declare-styleable>

    <declare-styleable name="MaterialCircleImageView">
        <attr name="borderWidth" format="dimension|reference" />
        <attr name="borderColor" format="color|reference" />
        <attr name="borderOverlay" format="boolean" />>
        <attr name="circleBackgroundColor" format="color|reference" />
    </declare-styleable>

    <declare-styleable name="MaterialView">
        <attr name="str" format="string" />
        <attr name="dimension" format="dimension" />
        <attr name="colorId" format="color" />
        <attr name="drawableId" format="color|reference" />
    </declare-styleable>

    <declare-styleable name="PageLoader">
        <attr name="loaderHeaderText" format="string" />
        <attr name="loaderPrimaryText" format="string" />
        <attr name="showShowAnimationView" format="boolean" />
    </declare-styleable>

</resources>